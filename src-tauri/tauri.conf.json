{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "coordtransform",
  "version": "0.1.0",
  "identifier": "coordtransform",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "title": "coordtransform",
        "width": 800,
        "height": 600,
        "resizable": false,
        "maximizable": false,
        "backgroundColor": "#f6f6f6"
      }
    ],
    "security": {
      "csp": null
    }
  },

  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
